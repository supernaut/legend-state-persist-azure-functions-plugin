{
  "name": "@supernaut/legend-state-persist-azure-functions-plugin",
  "version": "1.0.1",
  "description": "A plugin for persisting legend state in Azure Functions",
  "author": {
    "name": "Johannes Axner",
    "email": "johannes@axner.org",
    "url": "https://www.axner.org"
  },
  "engines": {
    "node": ">= 24.0.0",
    "pnpm": ">= 10.0.0"
  },
  "homepage": "https://github.com/supernaut/legend-state-persist-azure-functions-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supernaut/legend-state-persist-azure-functions-plugin.git"
  },

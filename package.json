    "url": "https://github.com/supernaut/legend-state-persist-azure-functions-plugin/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "build": "tsc",
